package lista2.trabalhoav1;

public class Exerc017 {
	public static void main(String[] args) {
		int n[] = new int[3];
		int a[] = new int[3];
		int x[] = new int[3];

		for (int i = 0; i < 3; i++) {
			n[i] = i + 1 ;
			for (int j = 0; j < 3; j++) {
				if (n[i] > a[j]) {
					x[i] = j + 1 ;
					n[i] = a[j];
					a[j] = x[i];
				}
			}
		}

		for (int i = 0; i < 3; i++) {
			System.out.println(n[i]);
			System.out.println("\n");
		}
		for (int i = 0; i < 3; i++) {
			System.out.println(a[i]);
			System.out.println("\n");
		}
	}
}
